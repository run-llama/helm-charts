env vars should match snapshot for default values with subcharts enabled:
  1: |
    - name: LLAMACLOUD_LICENSE_KEY
      valueFrom:
        secretKeyRef:
          key: llamacloud-license-key
          name: RELEASE-NAME-llamacloud-license-key
    - name: DATABASE_HOST
      value: RELEASE-NAME-postgresql
    - name: DATABASE_PORT
      value: "5432"
    - name: DATABASE_NAME
      value: llamacloud
    - name: DATABASE_USER
      value: llamacloud
    - name: DATABASE_PASSWORD
      valueFrom:
        secretKeyRef:
          key: password
          name: RELEASE-NAME-postgresql
    - name: MONGODB_HOST
      value: RELEASE-NAME-mongodb
    - name: MONGODB_PORT
      value: "27017"
    - name: MONGODB_USER
      value: root
    - name: MONGODB_PASSWORD
      valueFrom:
        secretKeyRef:
          key: mongodb-root-password
          name: RELEASE-NAME-mongodb
    - name: JOB_QUEUE_ENDPOINT
      value: amqp://RELEASE-NAME-llamacloud-rabbitmq:5672
    - name: JOB_QUEUE_USERNAME
      value: user
    - name: JOB_QUEUE_PASSWORD
      valueFrom:
        secretKeyRef:
          key: rabbitmq-password
          name: RELEASE-NAME-llamacloud-rabbitmq
    - name: REDIS_HOST
      value: RELEASE-NAME-llamacloud-redis-master
    - name: REDIS_PORT
      value: "6379"
